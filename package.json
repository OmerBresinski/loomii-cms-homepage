{
  "name": "ai-cms",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "db:generate": "pnpm --filter @ai-cms/db generate",
    "db:migrate": "pnpm --filter @ai-cms/db migrate:dev",
    "db:migrate:deploy": "pnpm --filter @ai-cms/db migrate:deploy",
    "db:push": "pnpm --filter @ai-cms/db push",
    "db:studio": "pnpm --filter @ai-cms/db studio",
    "db:seed": "pnpm --filter @ai-cms/db seed"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}

